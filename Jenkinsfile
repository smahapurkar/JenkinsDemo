pipeline {
    agent any

    stages {
         stage('JenkinsFile') {
            steps {
                echo 'This is run from jenkinsfile'
            }
        }
        stage('Build') {
            steps {
                echo 'Build is successfull'
            }
        }
        stage('Test') {
            steps {
                echo 'Test is successfull'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy is successfull'
            }
        }
    }
}
